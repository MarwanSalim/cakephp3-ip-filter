{
    "name": "tyrellsys/cakephp3-ip-filter",
    "description": "restrict access by ip address for CakePHP3",
    "type": "cakephp-plugin",
    "repositories": [
        {
            "type": "path",
            "url": "./packages/xrstf/ip-utils"
        }
    ],
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "~3.6",
        "jalle19/php-whitelist-check": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "replace": {
        "xrstf/ip-utils": "*"
    },
    "autoload": {
        "psr-4": {
            "Tyrellsys\\CakePHP3IpFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tyrellsys\\CakePHP3IpFilter\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
